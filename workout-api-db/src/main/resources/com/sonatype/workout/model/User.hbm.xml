<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 	<class name="com.sonatype.workout.model.User" table="users">
 	 	<id column="id" name="id" type="integer">
 	 		<generator class="native"/>
 	 	</id>
 	 	<property column="first_name" name="firstName" type="string"/>
 	 	<property column="last_name" name="lastName" type="string"/>
 	 	<property column="login" name="login" type="string"/>
 	 	<property column="password" name="password" type="binary"/>
 	</class>
</hibernate-mapping>